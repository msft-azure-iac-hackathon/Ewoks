{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
      "kvtTenantId": {
          "type": "string"
      },
      "kvtSkuName": {
          "type": "string",
          "allowedValues": [
              "standard",
              "premium"
          ],
          "defaultValue": "standard"
      },
      "kvtAccessPolicies": {
          "type": "array"
      },
      "kvtEnabledForDeployment": {
          "type": "bool",
          "defaultValue": true
      },
      "kvtEnabledForDiskEncryption": {
          "type": "bool",
          "defaultValue": true
      },
      "kvtEnabledForTemplateDeployment": {
          "type": "bool",
          "defaultValue": true
      },
      "kvtCount": {
          "type": "int",
          "defaultValue": 1
      },
      "kvtCustomerId": {
          "type": "string"
      }
    }
}